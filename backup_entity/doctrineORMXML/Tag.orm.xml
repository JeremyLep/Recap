<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Tag" table="tag">
    <id name="tagId" type="integer" column="tag_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="label" type="string" column="label" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-many field="idFiche" target-entity="Fiche" inversed-by="tag" fetch="LAZY">
      <join-table name="fiche_tag">
        <join-columns>
          <join-column name="tag_id" referenced-column-name="tag_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="id_fiche" referenced-column-name="fiche_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
