<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Ressource" table="ressource">
    <indexes>
      <index name="FK_Ressource_id_Fiche" columns="id_Fiche"/>
    </indexes>
    <id name="ressourceId" type="integer" column="ressource_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="titre" type="string" column="titre" length="75" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="routedoc" type="string" column="routeDoc" length="300" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idFiche" target-entity="Fiche" fetch="LAZY">
      <join-columns>
        <join-column name="id_Fiche" referenced-column-name="fiche_id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
