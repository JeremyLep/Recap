<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Commentaire" table="commentaire">
    <indexes>
      <index name="FK_commentaire_id_Fiche" columns="id_fiche"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idMembre" type="integer" column="id_membre" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="contenu" type="string" column="contenu" length="700" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateCom" type="date" column="date_com" nullable="true"/>
    <many-to-one field="idFiche" target-entity="Fiche" fetch="LAZY">
      <join-columns>
        <join-column name="id_fiche" referenced-column-name="fiche_id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
